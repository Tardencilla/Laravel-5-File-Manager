{
  "name": "infinety-es/filemanager",
  "description": "Laravel 5 Filemanager",
  "keywords": ["file", "manager", "laravel 5", "infinety"],
  "homepage": "https://github.com/Krato/Laravel-5-File-Manager",
  "license": "MIT",
  "authors": [
    {
      "name": "Eric Lagarda",
      "email": "eric@infinety.es"
    }
